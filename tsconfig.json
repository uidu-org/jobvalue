{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": false,
    "declaration": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "noUnusedParameters": false,
    "strict": false,
    "strictFunctionTypes": true,
    "target": "es5",
    "module": "esnext",
    "lib": [
      "dom",
      "es7",
      "scripthost",
      "es2015.core",
      "es2015.collection",
      "es2015.symbol",
      "es2015.iterable",
      "es2015.promise"
    ],
    "typeRoots": ["./typings", "./node_modules/@types"],
    "esModuleInterop": true
  }
}
