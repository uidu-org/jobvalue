{
  "name": "jobvalue",
  "private": true,
  "devDependencies": {
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.23",
    "@uidu/docs": "^0.1.39",
    "copy-pkg": "^1.0.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "                               run-p build:pkg build:typescript --print-label --continue-on-error",
    "build:pkg": "                           bolt workspaces exec --only-fs 'packages/**' -- copy-pkg package.json tmp/package.json --only name,version,sideEffects && bolt workspaces exec --only-fs 'packages/**' -- mv ./tmp/package.json ./version.json",
    "build:typescript": "                    NODE_ENV=production bolt workspaces exec --only-fs 'packages/**' -- tsc --project ./build/es2015",
    "build:website": "                       bolt install --pure-lockfile && cd website && yarn build",
    "changeset": "                           changeset",
    "start": "                               dotenv yarn run website:start",
    "typecheck:typescript": "                tsc --noEmit --project ./tsconfig.typecheck.json",
    "website:start": "                       cd website && yarn start"
  },
  "bolt": {
    "workspaces": [
      "packages/*/*",
      "themes/*",
      "website"
    ]
  },
