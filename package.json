{
  "name": "jobvalue",
  "private": true,
  "workspaces": [
    "packages/*/*",
    "themes/*",
    "website"
  ],
  "scripts": {
    "build": "                               yarn preconstruct build && run-p build:pkg",
    "build:pkg": "                           bolt workspaces exec --only-fs 'packages/**' -- copy-pkg package.json tmp/package.json --only name,version,sideEffects && bolt workspaces exec --only-fs 'packages/**' -- mv ./tmp/package.json ./version.json",
    "build:typescript": "                    NODE_ENV=production bolt workspaces exec --only-fs 'packages/**' -- tsc --project ./build/es2015",
    "build:website": "                       bolt install --pure-lockfile && cd website && yarn build",
    "changeset": "                           changeset",
    "delete": "                              yarn run delete:build && yarn run delete:tmp && yarn run delete:modules",
    "delete:build": "                        manypkg exec rm -rf dist",
    "delete:modules": "                      manypkg exec rm -rf node_modules && rm -rf node_modules",
    "delete:tmp": "                          manypkg exec rm -rf tmp",
    "postinstall": "                         manypkg check && preconstruct dev",
    "pkg:sort": "                            npx sort-package-json package.json && yarn manypkg exec npx sort-package-json package.json",
    "start": "                               dotenv yarn run website:start",
    "typecheck:typescript": "                tsc --noEmit",
    "website:start": "                       cd website && yarn start"
  },
