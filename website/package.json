{
  "name": "@jobvalue/website",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "node --max-old-space-size=8192 ./node_modules/@uidu/webpack-config/bin/build.js && yarn build:copy",
    "build:copy": "cp dist/index.html dist/404.html && cp -R public/* dist/",
    "build:no-minimize": "yarn build --no-minimize",
    "build:report": "yarn build --report",
    "predeploy": "yarn build",
    "deploy": "netlify deploy --dir dist/ --prod",
    "start": "node webpack.config.js"
  },
  "dependencies": {
    "@jobvalue/themes-stipendiogiusto": "^0.1.2",
    "@uidu/drawer": "^0.4.6",
    "@uidu/field-text": "^0.3.4",
    "@uidu/modal-dialog": "^0.3.4",
    "@uidu/navigation": "^0.4.3",
    "@uidu/select": "^0.5.4",
    "@uidu/shell": "^1.0.7",
    "@uidu/side-navigation": "^0.1.4",
    "@uidu/theme": "^0.4.4",
    "avataaars": "^1.2.1",
    "codesandboxer": "^1.0.3",
    "hoverintent": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "react": "^16.13.1",
    "react-codesandboxer": "^3.1.5",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.8",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.3.1",
    "react-media": "^1.10.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "sentence-case": "^3.0.3",
    "shallow-equal": "^1.2.1",
    "snake-case": "^3.0.3",
    "tti-polyfill": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "typescript": "^4.0.2"
  }
}
